{
  "name": "viomi-webpack-config",
  "version": "1.0.0",
  "main": "index.js",
  "author": "ttq",
  "license": "MIT",
  "scripts": {
    "dev": "node_modules/.bin/webpack-dev-server --config webpack.config.dev.js --open",
    "dev:sit": "cross-env env_config=sit node_modules/.bin/webpack-dev-server --config webpack.config.dev.js --open",
    "dev:uat": "cross-env env_config=uat node_modules/.bin/webpack-dev-server --config webpack.config.dev.js --open",
    "build": "cross-env env_config=prod node_modules/.bin/webpack --config webpack.config.prod.js",
    "analyze": "node_modules/.bin/webpack --config webpack.config.prod.js --json > stats.json"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "babel-loader": "^8.2.1",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.1.0",
    "cross-env": "^7.0.2",
    "css-loader": "^4.3.0",
    "css-minimizer-webpack-plugin": "^1.1.5",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^4.4.1",
    "less": "^3.12.2",
    "less-loader": "^7.0.1",
    "mini-css-extract-plugin": "^0.11.2",
    "sass": "^1.26.10",
    "sass-loader": "^10.0.2",
    "style-loader": "^1.2.1",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^4.1.0",
    "vue-loader": "^15.9.3",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "2.6.11",
    "webpack": "^4.44.1",
    "webpack-bundle-analyzer": "^4.1.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.4.0"
  },
  "dependencies": {}
}